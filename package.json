  "devDependencies": {
    "@types/commander": "^2.3.30",
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.40",
    "mocha": "^3.0.2",
    "nsp": "^2.6.1",
    "shx": "^0.2.1",
    "tslint": "^4.0.2",
    "typescript": "^2.1.0"
  },
  "engines": {
    "node": "~6.3.0"
  },
  "typings": "./lib/index.d.ts"
