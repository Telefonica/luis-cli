{
  "name": "luis-cli",
  "version": "1.0.0",
  "description": "CLI for Microsoft LUIS API",
  "license": "UNLICENSED",
  "repository": "https://github.com/Telefonica/yot-bot",
  "author": "TDAF",
  "keywords": [
    "LUIS",
    "bot",
    "fourth platform"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -fR lib ",
    "build": "npm run clean && npm run compile && npm run lint",
    "compile": "tsc -p .",
    "typings": "typings install",
    "lint": "tslint './src/**/*.ts'",
    "sec-deps": "nsp check",
    "postinstall": "npm run typings && npm run build",
    "test": "npm run build && mocha -R spec 'lib/**/*.spec.js'",
    "watch-ts": "nodemon --watch src -e ts --exec 'npm run compile'"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "request": "^2.74.0",
    "request-promise-native": "^1.0.3"
  },
  "devDependencies": {
    "nsp": "^2.6.1",
    "tslint": "^3.14.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  },
  "engines": {
    "node": "~6.3.0"
  }
}
